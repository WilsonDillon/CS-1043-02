
public class Exam2Review {
	public static void main(String[] args) {

		//2
		System.out.println("#2");
		String str1 = "*";
		for ( int i = 0; i < 5; i++) {
			for ( int x = 0; x < 1; x++ ) {
				System.out.print(str1);
				str1 = str1 + "*";
			}
			System.out.println();
		}

	}
}
